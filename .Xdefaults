!! colors
! Special
*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6

! black
*.color0:       #1d1f21
*.color8:       #969896

! red
*.color1:       #cc342b
*.color9:       #cc342b

! green
*.color2:       #198844
*.color10:      #198844

! yellow
*.color2:       #fba922
*.color11:      #fba922

! blue
*.color4:       #3971ed
*.color12:      #3971ed

! magenta
*.color5:       #a36ac7
*.color13:      #a36ac7

! cyan
*.color6:       #3971ed
*.color14:      #3971ed

! white
*.color7:       #c5c8c6
*.color15:      #ffffff

!! looks

URxvt.letterSpace:0
URxvt.lineSpace:0
URxvt.geometry: 92x24
URxvt.internalBorder: 4
URxvt.lineSpace: 0
URxvt.letterSpace: 0
URxvt.geometry: 92x24
URxvt.internalBorder: 4
! URxvt.internalBorder: 24
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.saveline: 2048
URxvt.urgentOnBell: true
URxvt.depth: 24
URxvt.iso14755: false

!! binds

URxvt.keysym.Control-V: eval:paste_clipboard
URxvt.keysym.Control-C: eval:selection_to_clipboard

URxvt.font:         xft:Source Code Pro:pixelsize=11:antialias=true
!!URxvt.font:       xft:Mono:pixelsize=11:antialias=true

!!Extensions

URxvt.perl-ext-common:  default,matcher,resize-font,url-select

!! Ext Configs

!! Matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button:   1

!!Resize Font
URxvt.resize-font.smaller:  C-j
URxvt.resize-font.bigger:   C-k
URxvt.resize-font.reset:    C-equal

!! Url select
URxvt.keysum.M-u:   perl:url-select:select_next
URxvt.url-select.launcher:  /usr/bin/xdg-open
URxvt.url-select.underline: true
